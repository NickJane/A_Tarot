<script type="text/javascript">
        function over(td, url) {
            
        }
        function out() { }

        function InsertSmiley(url) {
            var gifname = url.split('/');
            gifname = (gifname[gifname.length - 1]).split('.')[0];
            $("textarea").val($("textarea").val() + "[img]" + gifname + "[/img]");
            $("#smiletable").hide();
        }
        function ReplaceSmileOfTextArea() {
            $("textarea").each(function () {
                var _smilereg = /\[img]j_00\d\d\[\/img]/gi;
                var v = this.value.replace(_smilereg, function ($0, $1) {
                    return "<img src='http://www.taluolaile.com/Content/ueditor/dialogs/emotion/images/jx2/" +
                    $0.replace('[img]', '').replace('[/img]', '') + ".gif' />";
                });
                this.value = v;
            });
        }
        function showsmiletable(obj) {
            var tb = $('#smiletable');
            var offset = $(obj).offset();
            //alert(offset.top+"  "+ offset.left);
            tb.css({ top: offset.top+40, left: offset.left-400 });
            tb.show();
        }
        $("textarea").each(function () {
            $(this).after("<input type='button' value='插入表情' onclick=\"showsmiletable(this);\" />");
        });
</script>

